<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape
            android:shape="oval">
            <solid android:color="#33aaaaaa"/>
            <size
                android:width="20dp"
                android:height="20dp"/>
            <stroke
                android:width="1dp"
                android:color="#33e9e9e9"/>
        </shape>
    </item>
    <item
        android:bottom="14dp"
        android:left="14dp"
        android:right="14dp"
        android:top="14dp">
        <shape
            android:shape="oval">
            <solid android:color="#ffffff"/>
            <size
                android:width="20dp"
                android:height="20dp"/>
            <stroke
                android:width="1dp"
                android:color="#e9e9e9"/>
        </shape>
    </item>
</layer-list>
